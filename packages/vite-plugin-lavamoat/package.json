{
  "name": "vite-plugin-lavamoat",
  "version": "1.0.0",
  "description": "Secure your Vite applications with LavaMoat runtime protection - blocks eval injection, prototype pollution, and supply chain attacks",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vite",
    "plugin", 
    "lavamoat",
    "security",
    "eval",
    "prototype-pollution",
    "supply-chain",
    "runtime-protection",
    "crypto-safe",
    "zero-config"
  ],
  "author": "SSP Team",
  "license": "MIT",
  "dependencies": {
    "ses": "1.14.0",
    "lavamoat-core": "16.6.2",
    "@lavamoat/aa": "4.3.4"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "vite": "^7.0.0"
  },
  "engines": {
    "node": ">=22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RunOnFlux/ssp-wallet"
  }
}
